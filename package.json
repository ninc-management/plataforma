    "node": "12.22.8",
    "npm": "6.14.15"
  },
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "node server.js",
    "start:dev": "tsc --build backend/tsconfig.json --watch & nodemon ./server.js",
    "start:proxy": "ng serve --configuration dev --proxy-config proxy-config.json --disable-host-check",
    "build": "tsc --build backend/tsconfig.json && ng build --configuration production --aot",
    "test": "ng test --watch=false",
    "test:dev": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage --watch=false",
    "lint": "eslint './src/**/*.ts' server.js './backend/**/*.ts'",
    "lint:fix": "ng lint plataforma-nortan --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci && npm run test:coverage && codecov",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points --tsconfig \"./src/tsconfig.app.json\""
  },
